using System;
using System.Collections.Generic;

namespace metod_kolku_pati_podstring_se_povtoruva_vo_string
{
    class Program
    {
        void podstring(string zbor, string sub)
        {
            int brojacsub = 0;

            for (int i = 0; i < zbor.Length - sub.Length + 1; i++)
            {
                int brojac = 0;
                for (int j = 0; j < sub.Length; j++)
                {
                    if (sub[j] == zbor[i + j])
                        brojac++;

                }
                if (brojac == sub.Length)

                    brojacsub++;
            }

            Console.WriteLine(brojacsub);
        }

        static void Main(string[] args)
        {

            Console.WriteLine("vnesete zbor ");
            string zbor = Convert.ToString(Console.ReadLine());
            Console.WriteLine("vnesete pod string ");
            string sub = Convert.ToString(Console.ReadLine());
            

            Program obj = new Program();
            obj.podstring(zbor, sub);


        }
    }
            
}

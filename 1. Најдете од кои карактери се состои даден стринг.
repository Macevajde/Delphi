using System;
using System.Collections.Generic;

namespace ConsoleApp16
{
    class Program

    {
        void karakterivostring(string zbor)
        {
            List<char> karakteri = new List<char>();
            for (int j = 0; j < zbor.Length; j++)
            {
                karakteri.Add(zbor[j]);

            }
            Console.WriteLine("Stringot '" + zbor + "' se sostoi od " + zbor.Length + " karakteri tie se: ");
            for (int k = 0; k < karakteri.Count; k++)
            {
                Console.WriteLine(karakteri[k]);

            }
        }
        static void Main(string[] args)
        {
            List<string> str = new List<string>() { "Marjan", "Vajdeski" };
            for (int i = 0; i < str.Count; i++)
            {
                Program obj = new Program();

                obj.karakterivostring(str[i]);

            }
        }
    }
}
